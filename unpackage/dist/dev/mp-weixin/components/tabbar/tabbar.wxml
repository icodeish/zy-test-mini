<view class="tabbar_pages"><view class="uni-tabbar"><block wx:for="{{tabbarList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['tabbarSave',['$0',index],[[['tabbarList','',index]]]]]]]}}" class="tababr_item tabbar_find" bindtap="__e"><block wx:if="{{item.type==0}}"><view class="tabbar_icon"><block wx:if="{{item.pagePath==pagePath}}"><image class="select_img" src="{{item.icon_select}}" mode></image></block><block wx:else><image src="{{item.icon}}" mode></image></block></view></block><block wx:if="{{item.type==1}}"><view class="find_icon"><view class="icon"><block wx:if="{{item.pagePath==pagePath}}"><image class="select_img" src="{{item.icon_select}}" mode></image></block><block wx:else><image src="{{item.icon}}" mode></image></block></view></view></block><view class="{{['tababr_text',item.pagePath==pagePath?'textActive':'']}}">{{item.text}}</view><block wx:if="{{isShowNums&&item.nums>0}}"><view class="tag">{{item.nums}}</view></block></view></block></view></view>